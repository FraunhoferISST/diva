{
    "$schema": "https://json-schema.org/draft/2019-09/schema",
    "$id": "columnStatistics",
    "type": "object",
    "title": "Sample rows of a table",
    "properties": {
        "columnStatistics": {
            "type": "array",
            "title": "The columnstatistics Schema",
            "items": {
              "type": "object",
              "title": "The Items Schema",
              "required": [
                "numberOfRows",
                "maxNumberOfRows",
                "numberOfNumbers",
                "numberOfStrings",
                "numberOfNullElements",
                "dataType",
                "columnIndex",
                "completeness"
              ],
              "properties": {
                "numberOfNumbers": {
                  "type": "integer",
                  "title": "Number of Numbers",
                  "description": "the number of numbers in a vector",
                  "default": 0,
                  "examples": [
                    201
                  ]
                },
                "numberOfStrings": {
                  "type": "integer",
                  "title": "Number of Strings",
                  "description": "the number of strings in a vector",
                  "default": 0,
                  "examples": [
                    40
                  ]
                },
                "numberOfNullElements": {
                  "type": "integer",
                  "title": "NullElements",
                  "description": "Number of intentionally empty fields.",
                  "default": 0,
                  "examples": [
                    23
                  ]
                },
                "numberOfRows": {
                  "type": "integer",
                  "title": "Number of Rows",
                  "description": "the number of terms in a vector",
                  "default": 0,
                  "examples": [
                    228
                  ]
                },
                "maxNumberOfRows": {
                  "type": "integer",
                  "title": "Max number of rows",
                  "description": "the max number of terms the vector could have in regards to other vectors compared",
                  "default": 0,
                  "examples": [
                    230
                  ]
                },
                "dataType": {
                  "type": "string",
                  "title": "Data Type",
                  "description": "the data type used in a vector",
                  "default": 0,
                  "examples": [
                    "string",
                    "number",
                    "mixed"
                  ],
                  "pattern": "^(string|number|mixed)$"
                },
                "frequencyNumbers": {
                  "type": "array",
                  "title": "Frequency",
                  "description": "Array with frequency per number value",
                  "items": {
                    "type": "object",
                    "title": "Items",
                    "description": "the value and its number of appearance",
                    "required": [
                      "token",
                      "count"
                    ],
                    "properties": {
                      "token": {
                        "type": "number",
                        "title": "Token",
                        "default": "",
                        "examples": [
                          115
                        ]
                      },
                      "count": {
                        "type": "integer",
                        "title": "Count",
                        "default": 0,
                        "examples": [
                          1
                        ]
                      }
                    }
                  }
                },
                "frequencyStrings": {
                  "type": "array",
                  "title": "FrequencyStrings",
                  "description": "Array with frequency per string value",
                  "items": {
                    "type": "object",
                    "title": "Items",
                    "description": "the value and its number of appearance",
                    "required": [
                      "token",
                      "count"
                    ],
                    "properties": {
                      "token": {
                        "type": "string",
                        "title": "Token",
                        "default": "",
                        "examples": [
                          "115"
                        ]
                      },
                      "count": {
                        "type": "integer",
                        "title": "Count",
                        "default": 0,
                        "examples": [
                          1
                        ]
                      }
                    }
                  }
                },
                "minCharLength": {
                  "type": "integer",
                  "minimum": 0,
                  "title": "Minimum Character Length",
                  "description": "the number of characters of the shortest string in a vector",
                  "default": 0,
                  "examples": [
                    2
                  ]
                },
                "maxCharLength": {
                  "type": "integer",
                  "minimum": 0,
                  "title": "Maximal Character Length",
                  "description": "the number of characters of the longest string in a vector",
                  "default": 0,
                  "examples": [
                    312532
                  ]
                },
                "sum": {
                  "type": "number",
                  "title": "Sum",
                  "description": "the sum of a vector or matrix columns",
                  "default": 0,
                  "examples": [
                    26106
                  ]
                },
                "sumSquared": {
                  "type": "number",
                  "title": "Sum Squared",
                  "description": "the sum squared of a vector or matrix columns",
                  "default": 0,
                  "examples": [
                    3976814.12
                  ]
                },
                "sumSquaredError": {
                  "type": "number",
                  "title": "Sum Squared Error",
                  "description": "the sum of squared errors of prediction of a vector",
                  "default": 0,
                  "examples": [
                    987677
                  ]
                },
                "minValue": {
                  "type": "number",
                  "title": "minValue",
                  "description": "the minimum value of a vector",
                  "default": 0,
                  "examples": [
                    0.023
                  ]
                },
                "maxValue": {
                  "type": "number",
                  "title": "Max Value",
                  "description": "the maximum value of a vector",
                  "default": 0,
                  "examples": [
                    228.23
                  ]
                },
                "mean": {
                  "type": "number",
                  "title": "Mean Value",
                  "description": "the mean of a vector",
                  "default": 0,
                  "examples": [
                    114.5
                  ]
                },
                "meanSquaredError": {
                  "type": "number",
                  "title": "Mean Squared Error",
                  "description": "the mean squared error of a vector",
                  "default": 0,
                  "examples": [
                    4331.916666666667
                  ]
                },
                "median": {
                  "type": "number",
                  "title": "Median Value",
                  "description": "the median of a vector",
                  "default": 0,
                  "examples": [
                    114.5
                  ]
                },
                "variancePopulation": {
                  "type": "number",
                  "title": "Variance Population",
                  "description": "the variance of a vector (population)",
                  "default": 0,
                  "examples": [
                    4331.916666666667
                  ]
                },
                "varianceSample": {
                  "type": "number",
                  "title": "Variance Sample",
                  "description": "the variance of a vector (sample)",
                  "default": 0,
                  "examples": [
                    4351
                  ]
                },
                "standardDeviationPopulation": {
                  "type": "number",
                  "title": "Standard Deviation Population",
                  "description": "the standard deviation of a vector (population)",
                  "default": 0,
                  "examples": [
                    65.81729762506713
                  ]
                },
                "standardDeviationSample": {
                  "type": "number",
                  "title": "Standard Deviation Sample",
                  "description": "the standard deviation of a vector (sample)",
                  "default": 0,
                  "examples": [
                    65.96211033616193
                  ]
                },
                "meanDeviation": {
                  "type": "number",
                  "title": "Mean Deviation",
                  "description": "the mean absolute deviation of a vector",
                  "default": 0,
                  "examples": [
                    57.01
                  ]
                },
                "medianDeviation": {
                  "type": "number",
                  "title": "Median Deviation",
                  "description": "the median absolute deviation of a vector",
                  "default": 0,
                  "examples": [
                    57.22
                  ]
                },
                "skewness": {
                  "type": "number",
                  "title": "Skewness",
                  "description": "the skewness of a vector (third standardized moment)",
                  "default": 0,
                  "examples": [
                    -1.1686558153949016e-17
                  ]
                },
                "excessKurtosis": {
                  "type": "number",
                  "title": "Excess Kurtosis",
                  "description": "the excess kurtosis of a vector (fourth standardized moment - 3)",
                  "default": 0,
                  "examples": [
                    -1.2000461689398456
                  ]
                },
                "coefficientVariation": {
                  "type": "number",
                  "title": "Coefficient Variation",
                  "description": "the coefficient of variation of a vector",
                  "default": 0,
                  "examples": [
                    0.5748235600442544
                  ]
                },
                "quantileQ1": {
                  "type": "number",
                  "title": "Quantile Q1",
                  "description": "the 1-th percentile of values in a range",
                  "default": 0,
                  "examples": [
                    57.75
                  ]
                },
                "quantileQ2": {
                  "type": "number",
                  "title": "Quantile Q2",
                  "description": "the 2-th percentile of values in a range",
                  "default": 0,
                  "examples": [
                    114.5
                  ]
                },
                "quantileQ3": {
                  "type": "number",
                  "title": "Quantile Q3",
                  "description": "the 3-th percentile of values in a range",
                  "default": 0,
                  "examples": [
                    171.25
                  ]
                },
                "title": {
                  "type": "string",
                  "title": "Title",
                  "description": "Title of a vector or column",
                  "default": "",
                  "examples": [
                    "status",
                    "name",
                    "gender"
                  ],
                  "pattern": "^(.*)$"
                },
                "id": {
                  "type": "string",
                  "title": "Unique ID of a column",
                  "examples": [
                    "e3b0c44298fc1c149afbf4c8996fb92427ae41e4649b934ca495991b7852b855#1#col1"
                  ],
                  "pattern": "^(.*)$"
                },
                "columnIndex": {
                  "type": "number",
                  "title": "Column Index",
                  "description": "the index of the vector in a matrix",
                  "minimum": 0,
                  "examples": [
                    0
                  ]
                },
                "completeness": {
                  "type": "number",
                  "title": "Completeness",
                  "description": "Completeness of the row",
                  "default": 0,
                  "minimum": 0,
                  "maximum": 100,
                  "examples": [
                    0.76
                  ]
                }
              }
            }
          }
    }
}
