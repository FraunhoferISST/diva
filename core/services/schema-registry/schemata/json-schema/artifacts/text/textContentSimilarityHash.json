{
    "$schema": "https://json-schema.org/draft/2019-09/schema",

    "type": "object",
    "title": "Text Content Similarity hash",
    "properties": {
        "textContentSimilarityHash": {
            "type": "string",
            "description": "256bit Hash in Hex that is used to calculate similarity between texts",
            "minLength": 64,
            "maxLength": 64,
            "examples": [
                "7014004300020144902045244c145a8020110209005408081520802004021b06"
            ],
            "_elasticsearch": {
                "type": "keyword"
            }
        }
    }
}