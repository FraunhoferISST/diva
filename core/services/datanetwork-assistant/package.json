{
  "name": "datanetwork-assistant",
  "version": "0.1.0",
  "description": "This service provides a way of adding and deleting typed and directed edges in neo4j:4.3.7-community",
  "main": "index.js",
  "scripts": {
    "start": "NODE_ENV=production node index.js",
    "dev": "NODE_ENV=development nodemon index.js",
    "eslint-fix": "eslint --fix . --ext .js",
    "build:alpine": "pkg --config package.json -t node14-alpine-x64 --output index-alpine index.js",
    "build:linux": "pkg -t node14-linux-x64 index.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/FraunhoferISST/diva"
  },
  "keywords": [
    "datanetwork",
    "graph"
  ],
  "author": "dtebernum",
  "license": "ISC",
  "dependencies": {
    "@diva/common": "^2.0.0"
  },
  "devDependencies": {
    "@diva/eslint-config": "^0.1.0",
    "pkg": "^5.3.1"
  },
  "pkg": {
    "assets": [
      "package.json",
      "apiDoc"
    ]
  }
}