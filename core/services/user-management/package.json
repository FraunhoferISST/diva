{
  "name": "user-management",
  "version": "4.0.0-beta.7",
  "description": "User management service with CRUD operations for users",
  "main": "index.js",
  "scripts": {
    "start": "NODE_ENV=test node index.js",
    "dev": "NODE_ENV=development node index.js",
    "build:alpine": "pkg --config package.json -t node14-alpine-x64 --output index-alpine index.js",
    "build:linux": "pkg -t node14-linux-x64 index.js",
    "eslint-fix": "eslint --fix . --ext .js",
    "test:e2e": "NODE_ENV=test mocha --recursive test/e2e/*.spec.js",
    "test:unit": "NODE_ENV=test mocha --recursive test/unit/*.spec.js"
  },
  "author": "",
  "license": "ISST",
  "dependencies": {
    "@diva/common": "1.0.0",
    "bcryptjs": "^2.4.3",
    "hasha": "^5.2.2",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.21",
    "mongodb": "^3.6.4",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0"
  },
  "devDependencies": {
    "@diva/eslint-config": "0.1.0",
    "dotenv": "^10.0.0",
    "pkg": "^5.3.1"
  },
  "pkg": {
    "assets": [
      "package.json",
      "apiDoc"
    ]
  }
}
