{
    "$schema": "https://json-schema.org/draft/2019-09/schema",

    "type": "object",
    "title": "Number of Columns of a Table",
    "properties": {
        "tableCompleteness": {
            "type": "number",
            "title": "Number of columns",
            "description": "the number of existing columns in the csv file",
            "default": 0,
            "minimum": 0,
            "maximum": 1,
            "examples": [
                1,
                0.4
            ],
            "_elasticsearch": {
                "type": "float"
            }
        }
    }
}