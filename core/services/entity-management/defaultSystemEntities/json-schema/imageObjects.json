{
    "title": "Objects in an Image",
    "schemaName": "imageObjects",
    "scope": [
        {
            "key": "resourceType",
            "value": "file"
        }
    ],
    "systemEntityType": "schema",
    "schema": {
        "$schema": "https://json-schema.org/draft/2019-09/schema",

        "type": "object",
        "title": "Objects in an Image",
        "properties": {
            "imageObjects": {
                "type": "array",
                "minItems": 0,
                "title": "Objects detected in image",
                "description": "Objects of an image",
                "items": {
                    "type": "object",
                    "title": "Items",
                    "description": "guessed objects",
                    "required": [
                        "label",
                        "probability"
                    ],
                    "properties": {
                        "label": {
                            "type": "string",
                            "title": "Label",
                            "description": "Guessed object for image",
                            "examples": [
                                "person",
                                "knife",
                                "dog"
                            ]
                        },
                        "probability": {
                            "type": "number",
                            "title": "Probability",
                            "description": "Probability of the guessed objects",
                            "default": 0,
                            "examples": [
                                1,
                                0.9724364236,
                                0.90947
                            ],
                            "_elasticsearch": {
                                "type": "float"
                            }
                        }
                    }
                }
            }
        }
    }
}


