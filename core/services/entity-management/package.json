{
  "name": "entity-management",
  "version": "1.0.0",
  "description": "Manages metadata of entities and publishes CRUD events to kafka",
  "repository": {
    "url": "https://github.com/FraunhoferISST/diva"
  },
  "main": "index.js",
  "scripts": {
    "start": "NODE_ENV=production node index.js",
    "dev": "NODE_ENV=development POLICY_MIDDLEWARE=fale  nodemon index.js",
    "eslint-fix": "eslint --fix . --ext .js",
    "build:alpine": "pkg --config package.json -t node16-alpine-x64 --output index-alpine index.js",
    "build:linux": "pkg --config package.json -t node16-linux-x64 --output index-linux index.js",
    "test:e2e": "NODE_ENV=test mocha --recursive test/e2e/*.spec.js --colors=true"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@apidevtools/json-schema-ref-parser": "^9.0.9",
    "@diva/common": "3.0.0",
    "glob": "^7.2.0"
  },
  "devDependencies": {
    "@diva/eslint-config": "^0.1.0",
    "pkg": "^5.3.1"
  },
  "pkg": {
    "assets": [
      "package.json",
      "apiDoc",
      "defaultSystemEntities"
    ]
  }
}
