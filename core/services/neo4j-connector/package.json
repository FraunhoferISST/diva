{
  "name": "neo4j-connector",
  "version": "0.0.1",
  "description": "This service looks for entity events. Updates are propagated to an neo4j instance.",
  "repository": {
    "url": "https://github.com/FraunhoferISST/diva"
  },
  "main": "index.js",
  "scripts": {
    "start": "NODE_ENV=production node index.js",
    "dev": "NODE_ENV=development nodemon index.js",
    "eslint-fix": "eslint --fix . --ext .js",
    "build:alpine": "pkg --config package.json -t node14-alpine-x64 --output index-alpine index.js",
    "build:linux": "pkg -t node14-linux-x64 index.js"
  },
  "author": "Daniel Tebernum",
  "license": "Apache 2.0",
  "dependencies": {
    "@diva/common": "^1.1.2"
  },
  "devDependencies": {
    "@diva/eslint-config": "^0.1.0",
    "pkg": "^5.3.1"
  }
}